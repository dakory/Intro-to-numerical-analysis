   "source": [
    "import numpy as np\n",
    "from scipy.linalg import lu\n",
    "\n",
    "N = 5\n",
    "A = np.random.randint(1000, size=(N, N))\n",
    "P, L, U = lu(A)\n",
    "\n",
    "def dot_product(P, L, U):\n",
    "    return np.dot(P, np.dot(L, U))\n",
    "\n",
    "print(f\"A:\\n{A}\\n\\nP:\\n{P}\\n\\nL:\\n{L}\\n\\nU:\\n{U}\")\n",
    "print(f\"\\nDot product:\\n{dot_product(P, L, U)}\")"
   ]
